<mvc:View controllerName="at.clouddna.lagerverwaltung.controller.LieferungenDetails"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
	xmlns:page="sap.ui.core.mvc.Page"
    xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
	xmlns:fiori="sap.ui.core.mvc"
	
    xmlns="sap.m"
	xmlns:main="sap.ui.webc.fiori"
	height="100%">
    <Page id="lieferungen_page" title="Lieferungen">
        <content>
				
        </content>
        <footer>
            <Bar>
                <contentRight>
                    <Button id="btnSave" text="save" type="Accept"
                        icon="sap-icon://save" press="onSavePressed" visible="{editModel>/editmode}"/>
                    <Button id="btnDelete" text="delete" type="Reject"
                        icon="sap-icon://delete" press="onDeleteButtonPressed" visible="{editModel>/editmode}"/>
                </contentRight>
            </Bar>
        </footer>
    </Page>
</mvc:View>
